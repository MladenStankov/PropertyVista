{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "silently-remove-extras",
  "controllerPathGlobs": [
    "src/controllers/**/*.ts"
  ],
    "spec": {
      "outputDirectory": "public",
      "specVersion": 3
    },
    "securityDefinitions": {
      "cookieAuth": {
        "type": "apiKey",
        "in": "cookie",
        "name": "authToken"
      }
    },
    "routes": {
    "routesDir": "src/routes"
  }
  }